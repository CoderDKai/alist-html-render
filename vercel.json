{
    "headers": [
        {
            "source": "/dependency/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, s-maxage=86400, max-age=86400"
                }
            ]
        },
        {
            "source": "/style/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, s-maxage=86400, max-age=86400"
                }
            ]
        }
    ]
}